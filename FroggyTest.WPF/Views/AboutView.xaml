<Page x:Class="FroggyTest.WPF.Views.AboutView"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:FroggyTest.WPF.Views"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800" Background="White" FontSize="15"
      Title="About">

    <Grid>
        <StackPanel VerticalAlignment="Center" >
            <Image MaxWidth="150" Source="Resources/froggy.png"/>
            <TextBlock HorizontalAlignment="Center" Text="This is About page" Style="{StaticResource MaterialDesignBody1TextBlock}"/>
        </StackPanel>
    </Grid>
</Page>
